{
    "problemMatcher": [
        {
            "owner": "tslint",
            "pattern": [
                {
                    "regexp": "^(\\w+):\\s(.*):(\\d+):(\\d+) - (.*)$",
                    "severity": 1,
                    "file": 2,
                    "line": 3,
                    "column": 4,
                    "message": 5
                }
            ]
        }
    ]
}
